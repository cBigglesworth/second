*{
	padding: 0;
	margin: 0;
}
body{
	background-color: #eeeeee;
}
#dog{
	width: 100%;
	text-align: center;
	margin-top: 10%;
	font-size: 50px;
	display: block;	
}
#anle{
	width: 300px;
	height: 300px;
	background: -webkit-linear-gradient(top,#0189A3,#3BCFE9);
	border-radius: 50%;
	border: 5px solid grey;
	margin: 0 auto;
	margin-top: 8%;
	
}
#time,#answer{
	font-size: 36px;
	display: block;
	margin-top: 5%;
	text-align: center;
	line-height: 2em;
}
#answer{
	font-size: 32px;
	display: block;
	margin-top: 5% ;
	text-align: center;
	line-height: 2em;
	color: royalblue;
}
